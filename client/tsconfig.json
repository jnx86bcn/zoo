{
  "compilerOptions": {
    "sourceMap": false,
    "noImplicitAny": false,
    "module": "es6",
    "target": "ES6",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": false,
    "lib": [
      "dom",
      "es6"
    ],
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}   